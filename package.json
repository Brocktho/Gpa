{
  "name": "joe-flys",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:node": "cross-env NODE_ENV=development nodemon ./server.ts --watch ./server.ts",
    "dev:remix": "remix watch",
    "start": "cross-env NODE_ENV=production node ./server.ts",
    "test": "vitest",
    "deploy": "git add . && git commit -m 'new changes' && git push"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@prisma/client": "^4.1.0",
    "@remix-run/express": "^1.6.5",
    "@remix-run/node": "^1.6.5",
    "@remix-run/react": "^1.6.5",
    "@remix-run/serve": "^1.6.5",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node-cron": "^3.0.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.1",
    "prisma": "^4.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@remix-run/dev": "^1.6.5",
    "@remix-run/eslint-config": "^1.6.5",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.2.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.3",
    "@types/node": "^17.0.41",
    "@types/node-cron": "^3.0.2",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "binode": "^1.0.5",
    "c8": "^7.11.3",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "cypress": "^10.0.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^5.2.0",
    "msw": "^0.42.1",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "prisma": "^4.0.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.6",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.14",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.14.2"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "node --require tsconfig-paths/register prisma/seed.js"
  }
}
